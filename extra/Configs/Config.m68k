config TARGET_ARCH
	default "m68k"

config FORCE_OPTIONS_FOR_ARCH
	bool
	default y
	select ARCH_BIG_ENDIAN

config ARCH_CFLAGS
	string
	default "-Wa,--bitwise-or"

config LIBGCC_CFLAGS
	string
